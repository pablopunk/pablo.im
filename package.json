    "format": "bunx biome check --write src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/vercel": "^9.0.3",
    "@meltwater/fetch-favicon": "^1.0.4",
    "@supabase/postgrest-js": "^0.37.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "astro": "^5.16.9",
    "autoprefixer": "^10.2.6",
    "classnames": "^2.3.1",
    "dark-mode-code": "^1.0.0",
    "is-url": "^1.2.4",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.2.0",
    "react-supabase": "^0.2.0",
    "tailwindcss": "^3.3.2"
